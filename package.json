  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@nick12003/eslint-config-nick": "^1.0.1",
    "@types/cross-spawn": "^6.0.6",
    "@types/node": "^20.11.30",
    "@types/prompts": "^2.4.9",
    "@types/validate-npm-package-name": "^4.0.2",
    "@vercel/ncc": "^0.38.1",
    "child_process": "^1.0.2",
    "commander": "^12.0.0",
    "cross-spawn": "^7.0.3",
    "eslint": "^8.57.0",
    "fast-glob": "^3.3.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
